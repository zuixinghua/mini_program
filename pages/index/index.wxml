<!--index.wxml-->
<view class="container">
  <view class="map-container">
    <map
      id="myMap"
      style="width: 100%; height: 100%;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      scale="12"
      markers="{{markers}}"
      subkey=""
      enable-3D="true"
      show-compass="true"
      show-location
    ></map>
  </view>
</view>
